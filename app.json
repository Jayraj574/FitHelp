{
  "expo": {
    "name": "FitHelp",
    "slug": "FitHelp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SplashScreen.jpg",
    "scheme": "FitHelp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SplashScreen.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/SplashScreen.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/SplashScreen.jpg",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy":true
        }
      ],
    [
      "expo-font",
      {
        "fonts" : [
          "./assets/fonts/Rubik-Bold.ttf",
          "./assets/fonts/Rubik-ExtraBold.ttf",
          "./assets/fonts/Rubik-Medium.ttf",
          "./assets/fonts/Rubik-Light.ttf",
          "./assets/fonts/Rubik-Regular.ttf",
          "./assets/fonts/Rubik-SemiBold.ttf"
        ]
      }
    ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
